{% extends 'base.html.twig' %}

{% block body %}
<div class="my-3">
  <h1> Formations </h1>
</div>
  <div class="card my-3">
    <div class="card-body">
      {% for formation in formations %}
      <div class="card my-3">
        <div class="card-header">
          {{formation.nom}} :
        </div>
        <div class="card-body">
        {{formation.nomComplet}}
        </div>
      </div>
      {% endfor %}
    </div>
  </div>
{% endblock %}
