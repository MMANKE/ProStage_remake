<form method="post" action="{{ path('entreprise_delete', {'id': entreprise.id}) }}" onsubmit="return confirm('Etes vous sur de vouloir supprimer cette entreprise ?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ entreprise.id) }}">
    <input class="btn btn-primary" type="submit" value="Supprimer">
</form>
